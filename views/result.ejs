<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results</title>
</head>
<body>
    <h1>Result</h1>
    <% if (locals.error) { %>
        <p><%= error %></p>
    <% } else { %>
        <h2>Place: <%= place %></h2>
        <h3>Zip Codes:</h3>
        <ul>
            <% zips.forEach(function(zip) { %>
                <li><%= zip %></li>
            <% }); %>
        </ul>
    <% } %>
    <a href="/">Back to Search</a>
</body>
</html>